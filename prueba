import pandas as pd

# Cargar el archivo Excel con la fila 4 (Ã­ndice 3) como los encabezados
data = pd.read_excel('C:/Users/diego/Desktop/PROYECTO_MINERIA/PROYECTOFINAL/PERU_SINIESTROS.xlsx', header=3)

# Limpiar los nombres de las columnas (eliminar espacios adicionales)
data.columns = data.columns.str.strip()

# Verificar las primeras filas para comprobar que los datos y los encabezados son correctos
print("Primeras filas del archivo:")
print(data.head(10))

# Verificar los nombres de las columnas
print("\nNombres de las columnas:")
print(data.columns)

